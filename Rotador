#include <Servo.h>

Servo servo1;

int PINSERVO = 9;

void setup(){
  servo1.attach(PINSERVO);
}

void loop(){
  servo1.write(0);
  delay(1500);
  servo1.write(90);
  delay(1500);
 servo1.write(0);
  delay(1500);
  servo1.write(90);
  delay(1500);
 servo1.write(0);
  delay(1500);
  servo1.write(90);
  delay(1500);
 servo1.write(0);
  delay(1500);
  servo1.write(90);
  delay(1500);
  servo1.write(0);
  delay(21600000);
}
